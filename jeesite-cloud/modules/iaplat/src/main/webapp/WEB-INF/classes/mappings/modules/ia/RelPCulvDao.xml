<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jeesite.modules.ia.dao.RelPCulvDao">

    <insert id="insert">
		INSERT INTO REL_P_CULV(
			culcd,
			pid
		) VALUES (
			#{culcd},
			#{pid}
		)
	</insert>


    <update id="delete">
		DELETE FROM REL_P_CULV
		WHERE culcd = #{culcd}
	</update>
</mapper>