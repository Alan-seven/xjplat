<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jeesite.modules.ia.dao.RelPFloodDao">

    <insert id="insert">
		INSERT INTO REL_P_FLOOD(
			fcd,
			pid
		) VALUES (
			#{fcd},
			#{pid}
		)
	</insert>


    <update id="delete">
		DELETE FROM REL_P_FLOOD
		WHERE fcd = #{fcd}
	</update>
</mapper>